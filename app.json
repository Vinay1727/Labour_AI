{
  "expo": {
    "name": "Labour Chowk",
    "slug": "labour-chowk",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "assets/images/logo.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "assets/images/logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "assets/images/logo.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "assets/favicon.png"
    },
    "plugins": [
      "expo-asset",
      "expo-font",
      "expo-secure-store"
    ]
  }
}
